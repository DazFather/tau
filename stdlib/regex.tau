lexer = import("lexer")

alternation				   = 0
anchoring				   = 1
concatenation			   = 2
singleCharacterDuplication = 3
grouping				   = 4
bracketExpression		   = 5
escapedCharacters		   = 6
collationBracket		   = 7

Match = fn(input) {
	items = lexer.Lex(input)
	
	for (i = recv(items)) != null {
		if failed(i) {
			return i
		}
		println(i.String())
	}
}
